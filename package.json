{
  "name": "fan7-bn-backend",
  "version": "1.0.0",
  "description": "A web application intend to make travel and accommodation easy and convenient.",
  "main": "./src/server.js",
  "dependencies": {
    "@babel/register": "^7.8.3",
    "@hapi/joi": "^17.1.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.19.0",
    "connect-multiparty": "^2.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.2",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "redis": "^2.8.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "mocha": "^7.0.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.0.0",
    "sequelize-migration-pg-extras": "^1.0.1"
  },
  "scripts": {
    "pre-test": "cross-env NODE_ENV=test npm run undo:db && cross-env NODE_ENV=test npm run migrate:db && cross-env NODE_ENV=test npm run seed:db",
    "test": "npm run pre-test && cross-env NODE_ENV=test nyc --reporter=text --reporter=html mocha --require @babel/polyfill --require @babel/register --exit ./src/test/index.test.js",
    "start": "babel-node ./src/server.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "dev": "node_modules/.bin/nodemon ./src/server.js --exec babel-node --",
    "stop-win": "Taskkill /IM node.exe /F",
    "stop-linux": "killall node",
    "db:connect": "babel-node ./src/database/config",
    "create:db": "cross-env NODE_ENV=development && node_modules/.bin/sequelize db:create",
    "create:tables": "cross-env NODE_ENV=development && npm run migrate:db && npm run seed:db",
    "drop:db": "cross-env NODE_ENV=development && node_modules/.bin/sequelize db:drop",
    "migrate:db": "node_modules/.bin/sequelize db:migrate",
    "seed:db": "node_modules/.bin/sequelize db:seed:all",
    "undo:db": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stackup-Rwanda/fan7-bn-backend.git"
  },
  "keywords": [],
  "author": "",
